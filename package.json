{
  "name": "github-old-feed-userscript",
  "version": "0.0.0",
  "description": "Simply use `github.com/dashboard-feed` to bring back the old GitHub feed",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --fix --ignore-path .gitignore",
    "release": "npx standard-version && git push --follow-tags"
  },
  "keywords": [
    "userscript",
    "gihtub",
    "feed"
  ],
  "author": "Michael Wang 汪東陽 <michael19920327@gmial.com> (https://github.com/EastSun5566)",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "simple-git-hooks": "^2.9.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.js": "pnpm run lint"
  }
}
